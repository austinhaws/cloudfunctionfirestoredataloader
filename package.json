{
  "name": "cloud-function-firestore-data-loader",
  "version": "1.0.0",
  "description": "cloud function firestore data loader",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "cleaninstall_mac_linux": "./cleaninstall_mac.sh",
    "cleaninstall_win": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./cleaninstall_win.ps1",
    "build": "parcel build src/index.js --target node --detailed-report",
    "lint": "xo",
    "lint and fix": "xo --fix",
    "gcp-build": "npm run-script build",
    "serve": "nodemon -r dotenv/config dist/index.js",
    "npm reinstall": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "parcel src/index.js --target node"
  },
  "engines": {
    "node": "12"
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/filename-case": 1
    }
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "firebase-functions-test": "^0.2.1",
    "firebase-tools": "^8.4.3",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "xo": "^0.32.0"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.2.0",
    "@google-cloud/error-reporting": "^2.0.0",
    "@google-cloud/pubsub": "^2.2.0",
    "apollo-server-express": "^2.15.1",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.7.0",
    "graphql": "^14.2.1",
    "jwt-decode": "^3.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "openid-client": "^3.15.5",
    "uuid": "^8.3.0"
  }
}
